@using PkiSuiteAspNetMvcSample.Models.Sdk
@model SignatureInfoModel

<h2 class="ls-title">Sign a XML file stored on the server with PKI SDK</h2>

<p>File signed successfully!</p>

<h3>Actions:</h3>
<ul>
	<li><a href="/Download/File/@Model.File">Download the signed file</a></li>
	<li><a href="XmlDiplomaSignatureSdk/IndexRegistro?userfile=@Model.File">Continue signature process</a></li>
</ul>