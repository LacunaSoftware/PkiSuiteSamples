@using PkiSuiteAspNetMvcSample.Models.Sdk
@model SignatureInfoModel

<h2 class="ls-title">Merge CMS/CAdES signatures with PKI SDK</h2>

<p>Files merged successfully!</p>

<h3>Actions:</h3>
<ul>
	<li><a href="/Download/File/@Model.File">Download the merged file</a></li>
	<li><a href="/OpenCadesSignatureSdk?userfile=@Model.File">Open/validate the merged file</a></li>
	<li><a href="/CadesSignatureSdk?cmsfile=@Model.File">Co-sign with another certificate</a></li>
</ul>
